{
	"name": "kaanalnet",
	"description": "Virtual Network Emulator Lab for SDN and traditional networks ",
	"version": "0.0.1",
	"author": "Suresh Kumar",	
	"repository": {
		"type": "git",
		"url": "git://github.com/sureshkvl/kaanalnet.git"
	},
	"dependencies": {
	"restify":"4.0.0",
	"minimist":"1.1.3",	
	"stormdata": "1.1.7",
	"stormregistry": "1.1.8",	
	"request-json":"0.5.3",
	"ip":"0.3.2",
	"async":"1.4.2",	
	"log":"1.4.0"
	},
	"devDependencies": {
		"coffee-script": ">=1.9"
	},
	"config":{
		"debianpackages":{
